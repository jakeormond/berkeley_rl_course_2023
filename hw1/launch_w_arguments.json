{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "--expert_policy_file",
                "cs285/policies/experts/Ant.pkl",
                "--expert_data",
                "cs285/expert_data/expert_data_Ant-v4.pkl",
                "--env_name",
                "Ant-v4",
                "--exp_name",
                "Ant_expt_1"
            ]
        }
    ]
}